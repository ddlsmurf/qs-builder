Defined by <%= mediator_def.parent.id == QS::BUNDLE_ID ? "''Quicksilver internal''" : qs_object_link(mediator_def.parent, :tech) %>
<% 
handlers = QS::Registry.registrations(mediator_kind).sort.map { |id, value| [value.parent, value.name || id] }
if handlers.empty?
 %>
'''Handled nowhere'''
<% else
handlers.each do |plugin, name| %>
* <%= name %> in <%= plugin.id == QS::BUNDLE_ID ? "''Quicksilver internal''" : qs_object_link(plugin, :tech) %><%
 end
 end %>
<% end %>


